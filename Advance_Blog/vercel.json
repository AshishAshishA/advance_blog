{
    "version": 2,
    "builds": [
      {
        "src": "poll/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"20mb","runtime":"python3.11"}
      },
      [{"src": "build_files.sh",
        "dest": "@vercel/static-build",
        "config":{"distDir":"staticfiles_build"}}]
    ],
    "routes": [
      {
        "src":"static/(.*)",
        "dest":"static/$1"
      },
      {
        "src":"/(.*)",
        "dest":"poll/wsgi.py"
      }
    ]
  }
  